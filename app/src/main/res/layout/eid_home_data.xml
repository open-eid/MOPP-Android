<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:parentTag="android.widget.LinearLayout"
    tools:layout_width="match_parent"
    tools:layout_height="match_parent"
    tools:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:focusable="true"
        android:orientation="vertical">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/material_content_edge_margin_horizontal"
            android:paddingStart="@dimen/material_content_edge_margin_horizontal"
            android:paddingEnd="@dimen/material_content_edge_margin_horizontal"
            android:textAppearance="@style/TextAppearance.Application.Label"
            android:textAllCaps="false"
            android:text="@string/eid_home_data_type"
            android:contentDescription="@string/eid_home_data_type" />

        <TextView
            android:id="@id/eidHomeDataType"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingStart="@dimen/material_content_edge_margin_horizontal"
            android:paddingEnd="@dimen/material_content_edge_margin_horizontal"
            android:textAppearance="@style/TextAppearance.Application.Value" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:focusable="true"
        android:orientation="vertical">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/material_baseline_grid_1x"
            android:paddingStart="@dimen/material_content_edge_margin_horizontal"
            android:paddingEnd="@dimen/material_content_edge_margin_horizontal"
            android:textAppearance="@style/TextAppearance.Application.Label"
            android:labelFor="@id/eidHomeDataGivenNames"
            android:text="@string/eid_home_data_given_names"
            android:contentDescription="@string/eid_home_data_given_names_accessibility"/>

        <TextView
            android:id="@id/eidHomeDataGivenNames"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingStart="@dimen/material_content_edge_margin_horizontal"
            android:paddingEnd="@dimen/material_content_edge_margin_horizontal"
            android:textAppearance="@style/TextAppearance.Application.Value" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:focusable="true"
        android:orientation="vertical">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/material_baseline_grid_1x"
            android:paddingStart="@dimen/material_content_edge_margin_horizontal"
            android:paddingEnd="@dimen/material_content_edge_margin_horizontal"
            android:textAppearance="@style/TextAppearance.Application.Label"
            android:labelFor="@id/eidHomeDataSurname"
            android:text="@string/eid_home_data_surname"
            android:contentDescription="@string/eid_home_data_surname_accessibility"/>

        <TextView
            android:id="@id/eidHomeDataSurname"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingStart="@dimen/material_content_edge_margin_horizontal"
            android:paddingEnd="@dimen/material_content_edge_margin_horizontal"
            android:textAppearance="@style/TextAppearance.Application.Value" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:focusable="true"
        android:orientation="vertical">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/material_baseline_grid_1x"
            android:paddingStart="@dimen/material_content_edge_margin_horizontal"
            android:paddingEnd="@dimen/material_content_edge_margin_horizontal"
            android:textAppearance="@style/TextAppearance.Application.Label"
            android:labelFor="@id/eidHomeDataPersonalCode"
            android:text="@string/eid_home_data_personal_code" />

        <TextView
            android:id="@id/eidHomeDataPersonalCode"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingStart="@dimen/material_content_edge_margin_horizontal"
            android:paddingEnd="@dimen/material_content_edge_margin_horizontal"
            android:textAppearance="@style/TextAppearance.Application.Value" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:focusable="true"
        android:orientation="vertical">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/material_baseline_grid_1x"
            android:paddingStart="@dimen/material_content_edge_margin_horizontal"
            android:paddingEnd="@dimen/material_content_edge_margin_horizontal"
            android:textAppearance="@style/TextAppearance.Application.Label"
            android:labelFor="@id/eidHomeDataCitizenship"
            android:text="@string/eid_home_data_citizenship"
            android:contentDescription="@string/eid_home_data_citizenship_accessibility" />

        <TextView
            android:id="@id/eidHomeDataCitizenship"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingStart="@dimen/material_content_edge_margin_horizontal"
            android:paddingEnd="@dimen/material_content_edge_margin_horizontal"
            android:textAppearance="@style/TextAppearance.Application.Value" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:focusable="true"
        android:orientation="vertical">

        <TextView
            android:id="@id/eidHomeDataDocumentNumberLabel"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/material_baseline_grid_1x"
            android:paddingStart="@dimen/material_content_edge_margin_horizontal"
            android:paddingEnd="@dimen/material_content_edge_margin_horizontal"
            android:textAppearance="@style/TextAppearance.Application.Label"
            android:labelFor="@id/eidHomeDataDocumentNumber"
            android:text="@string/eid_home_data_document_number"
            android:contentDescription="@string/eid_home_data_document_number_accessibility" />

        <TextView
            android:id="@id/eidHomeDataDocumentNumber"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingStart="@dimen/material_content_edge_margin_horizontal"
            android:paddingEnd="@dimen/material_content_edge_margin_horizontal"
            android:textAppearance="@style/TextAppearance.Application.Value" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:focusable="true"
        android:orientation="vertical">

        <TextView
            android:id="@id/eidHomeDataExpiryDateLabel"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/material_baseline_grid_1x"
            android:paddingStart="@dimen/material_content_edge_margin_horizontal"
            android:paddingEnd="@dimen/material_content_edge_margin_horizontal"
            android:textAppearance="@style/TextAppearance.Application.Label"
            android:labelFor="@id/eidHomeDataExpiryDate"
            android:text="@string/eid_home_data_expiry_date"
            android:contentDescription="@string/eid_home_data_expiry_date_accessibility" />

        <TextView
            android:id="@id/eidHomeDataExpiryDate"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingStart="@dimen/material_content_edge_margin_horizontal"
            android:paddingEnd="@dimen/material_content_edge_margin_horizontal"
            android:textAppearance="@style/TextAppearance.Application.Value" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginTop="@dimen/material_baseline_grid_1x"
        android:background="@color/divider" />

    <TextView
        android:id="@id/eidHomeDataCertificatesTitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingStart="@dimen/material_content_edge_margin_horizontal"
        android:paddingEnd="@dimen/material_content_edge_margin_horizontal"
        android:drawablePadding="@dimen/material_baseline_grid_1x"
        android:minHeight="@dimen/material_button_touch_target_height_minimum"
        android:gravity="center_vertical"
        android:background="?attr/selectableItemBackground"
        android:textStyle="bold"
        android:text="@string/eid_home_data_certificates_title"
        android:contentDescription="@string/eid_home_data_certificates_title_accessibility"
        android:drawableStart="@drawable/ic_icon_accordion_collapsed" />

    <net.cachapa.expandablelayout.ExpandableLayout
        android:id="@id/eidHomeDataCertificatesContainer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:el_expanded="false">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/material_baseline_grid_1x"
            android:paddingStart="@dimen/material_content_edge_margin_horizontal"
            android:paddingEnd="@dimen/material_content_edge_margin_horizontal"
            android:paddingBottom="@dimen/material_baseline_grid_1x"
            android:orientation="vertical">

            <ee.ria.DigiDoc.android.eid.CertificateDataView
                android:id="@id/eidHomeDataCertificatesAuth"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:layout_marginTop="@dimen/material_baseline_grid_1x"
                android:layout_marginBottom="@dimen/material_baseline_grid_1.5x"
                android:background="@color/divider" />

            <ee.ria.DigiDoc.android.eid.CertificateDataView
                android:id="@id/eidHomeDataCertificatesSign"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:layout_marginTop="@dimen/material_baseline_grid_1x"
                android:layout_marginBottom="@dimen/material_baseline_grid_1.5x"
                android:background="@color/divider" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:focusable="true"
                android:orientation="vertical">

                <TextView
                    android:id="@id/eidHomeDataCertificatesPukTitle"
                    style="@style/Widget.Application.EID.Title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/eid_home_data_certificates_puk_title" />

                <TextView
                    android:id="@id/eidHomeDataCertificatesPukText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:text="@string/eid_home_data_certificates_puk_text" />
            </LinearLayout>

            <Button
                android:id="@id/eidHomeDataCertificatesPukButton"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/material_baseline_grid_1x"
                android:text="@string/eid_home_data_certificates_puk_button"
                android:contentDescription="@string/eid_home_data_certificates_puk_button_accessibility" />

            <TextView
                android:id="@id/eidHomeDataCertificatesPukError"
                style="@style/Widget.Application.EID.Error"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:focusable="true"
                android:text="@string/eid_home_data_certificates_puk_error"
                android:contentDescription="@string/eid_home_data_certificates_puk_error" />

            <TextView
                android:id="@id/eidHomeDataCertificatesPukLink"
                style="@style/Widget.Application.EID.Link"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/eid_home_data_certificates_puk_link" />

        </LinearLayout>

    </net.cachapa.expandablelayout.ExpandableLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginBottom="@dimen/material_content_edge_margin_horizontal"
        android:background="@color/divider" />

</merge>
